   "source": [
    "import numpy as np"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "[0 0 1 1] [1 1 0 0] 0.5\n"
     ]
    }
   ],
   "source": [
    "Y = np.array([0, 0, 1, 1])\n",
    "Y_ = np.array([0, 0, 1, 1])\n",
    "TP = Y * Y_\n",
    "FN = (1- Y) * (1 - Y_)\n",
    "recall = float(sum(TP))/ ((sum(TP) + sum(FN)))\n",
    "print TP, FN, recall"
   ]
